@using DNSLab.Pages.Errors
<Router AppAssembly="@typeof(Program).Assembly" PreferExactMatches="@true">
    <Found Context="routeData">
        <RouteView RouteData="@routeData" DefaultLayout="@typeof(MainLayoutNoAuth)">
        </RouteView>
    </Found>
    <NotFound>
        <Error404 />
    </NotFound>
</Router>