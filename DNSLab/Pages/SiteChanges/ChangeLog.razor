@page "/changelog/{*URL}"

<MudContainer MaxWidth="MaxWidth.Large" Class="ck-content">
    @if (_ChangeLog != null)
    {
        <MudText Typo="Typo.h1">@_ChangeLog.Title</MudText>
        <PageVisitCounter />
        <MudDivider />

        @((MarkupString)@_ChangeLog.Description)

        <SupportUsButton />
    }
</MudContainer>
